[run]
branch = True
package = 
    savReaderWriter

[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain about missing debug-only code:
    def __repr__
    if debug

    # Don't complain if non-runnable code isn't run:
    if __name__ == .__main__.:
    
omit = 
    *numpy*
    *_version*
